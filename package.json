{
  "name": "@aotearoan/neon",
  "description": "Neon is a lightweight design library of Vue 3 components with minimal dependencies.",
  "version": "9.2.1",
  "main": "./dist/neoon.cjs.js",
  "module": "dist/neon.es.js",
  "types": "dist/neon.d.ts",
  "exports": {
    ".": {
      "import": "./dist/neon.es.js",
      "require": "./dist/neon.cjs.js"
    },
    "./theme": "./src/sass/theme.scss",
    "./sass/includes/*": "./src/sass/includes/*"
  },
  "scripts": {
    "dev": "node doc-gen.js && genversion --es6 --semi ./version.ts && vite",
    "build": "genversion --es6 --semi ./version.ts && vite build && vue-tsc --emitDeclarationOnly",
    "test": "genversion --es6 --semi ./version.ts && jest --no-cache",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:scss": "stylelint ./src/**/*.scss"
  },
  "sideEffects": false,
  "files": [
    "package.json",
    "dist/neon.*",
    "dist/_virtual/**",
    "dist/common/**",
    "dist/components/**",
    "src/sass/**"
  ],
  "keywords": [
    "sass",
    "css",
    "vue",
    "VueJS",
    "Vue3",
    "design",
    "system",
    "component",
    "library",
    "dark",
    "light",
    "theme",
    "theming",
    "mode",
    "web",
    "SASS",
    "scss",
    "css",
    "variables",
    "application",
    "javascript",
    "typescript",
    "framework",
    "html",
    "neon",
    "lightweight",
    "ui",
    "ux"
  ],
  "dependencies": {
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^6.6.1",
    "@types/jest": "^28.1.8",
    "@types/jsdom": "21.1.0",
    "@types/jsdom-global": "^3.0.4",
    "@types/node": "^16.18.18",
    "@types/prismjs": "^1.26.0",
    "@types/vue": "^2.0.0",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.1",
    "@vue/tsconfig": "^0.1.3",
    "@vue/vue3-jest": "^29.2.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "genversion": "^3.1.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-serializer-vue": "^3.1.0",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "21.1.1",
    "jsdom-global": "3.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.5",
    "prismjs": "^1.29.0",
    "sass": "^1.59.3",
    "ts-jest": "^29.0.5",
    "typescript": "~4.7.4",
    "vite": "^2.9.15",
    "vite-plugin-css-injected-by-js": "^2.4.0",
    "vite-svg-loader": "^4.0.0",
    "vue-docgen-api": "^4.64.1",
    "vue-prism-editor": "^2.0.0-alpha.2",
    "vue-tsc": "^0.40.13",
    "vue3-runtime-template": "^1.0.2"
  },
  "author": "Andrew Erickson <andrew@aotearoan.com>",
  "license": "MIT",
  "repository": "https://github.com/aotearoan/neon.git",
  "bugs": {
    "url": "https://github.com/aotearoan/neon/issues"
  }
}
